<div class="col m9"><br>
  <center><h1 class="white-text"> Store's Pending Orders </h1></center>
  <div class="card">
    <table class="bordered">
      <thead>
        <tr>
          <th colspan="1"></th>
          <th>Name</th>
          <th>Description</th>
          <th>
            Winning Customer
          </th>
          <th> Final Price</th>
        </tr>
      </thead>
      <tbody>
        <% @store.orders.each do |order| %>
        <tr id="row_item_<%= order.item.id %>">
          <td><%= image_tag(order.item.image_url, class: 'item_cart') %></td>
          <td>
            <%= link_to order.item.name, item_path(order.item) %><br>
          </td>
          <td><%= order.item.description %></td>
          <td>
            <%= order.item.winning_bid.first.user.first_name %>
          </td>
          <td> $<%= order.amount %></td>
        </tr>
        <% end %>
      </tbody>
    </table>
  </div>
</div>
